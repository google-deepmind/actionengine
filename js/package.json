{
  "name": "@helenapankov/actionengine",
  "version": "0.1.7",
  "author": "Google DeepMind <no-reply@google.com>",
  "license": "Apache-2.0",
  "description": "A TypeScript client library for Action Engine.",
  "type": "module",
  "main": "build/main.js",
  "module": "build/main.js",
  "exports": {
    ".": "./build/main.js",
    "./*": "./build/src/*.js"
  },
  "types": "main.ts",
  "engines": {
    "node": ">= 23.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/eslint__js": "~8.42",
    "@types/node": "^20.19.17",
    "@typescript-eslint/parser": "~8.19",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "~9.32",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.14",
    "prettier": "~3.4",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "typescript": "~5.7",
    "typescript-eslint": "~8.19",
    "vitest": "~2.1"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:bundle": "npm run clean && tsc -p tsconfig.bundle.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "build:typings": "tsc --declaration --emitDeclarationOnly --outDir build",
    "lint": "eslint .",
    "test": "vitest run unit --config __tests__/vitest.config.ts",
    "test:coverage": "vitest run unit --config __tests__/vitest.config.ts --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest unit"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.1",
    "async-mutex": "^0.5.0",
    "tslib": "~2.8",
    "uuid": "^11.1.0"
  },
  "volta": {
    "node": "22.12.0"
  }
}
